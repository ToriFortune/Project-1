	//GET DATA
	function get_data(){
		if(m.dev_data==true || helper.config.dev==true){
			
		}else{
			get_data_ajax(get_data_callback,'day');
			get_data_ajax(get_data_callback,'week');
			get_data_ajax(get_data_callback,'month');
		};
	};
	//AJAX
	function get_data_ajax(callback,period){
		ajax.get(m.query_url, null, { //url, options, data, callback
			artist:helper.query_format(m.query_artist),
			album:helper.query_format(m.query_album),
			track:helper.query_format(m.query_track),
			username:helper.query_format(m.query_user),
			limit:m.length,
			period:period
		}, function(a,b,c,d){
			if(m.logs) console.log('--- ajax response - 2.1 trends - '+period+' ---');
			if(m.logs) console.log(b);
			if(typeof(b.results)!=='undefined' && b.results.length>0 &&typeof(callback)==='function'){
				b.results.reverse();
				callback(period,b.results);
			};
		});
	};
	//CALLBACKS
	function get_data_callback(period,b){
		//SAVE
		data[period] = b;
		//MATHS - MAX & MIN
		for(var i in data[period]){
			//user - wave - min
			if(m[period].min_user_wave==null || data[period][i].user_scrobbles<m[period].min_user_wave){
				m[period].min_user_wave = data[period][i].user_scrobbles;
			};
			//user - wave - max
			if(m[period].max_user_wave==null || data[period][i].user_scrobbles>m[period].max_user_wave){
				m[period].max_user_wave = data[period][i].user_scrobbles;
			};
			//user - bar - min
			if(helper.isOdd(i)){
				if(m[period].min_user_bar==null || (data[period][i].user_scrobbles + data[period][i-1].user_scrobbles)<m[period].min_user_bar){
					m[period].min_user_bar = data[period][i].user_scrobbles + data[period][i-1].user_scrobbles;
				};
			};
			//user - bar - max
			if(helper.isOdd(i)){
				if(m[period].max_user_bar==null || (data[period][i].user_scrobbles + data[period][i-1].user_scrobbles)>m[period].max_user_bar){
					m[period].max_user_bar = data[period][i].user_scrobbles + data[period][i-1].user_scrobbles;
				};
			};
			//global - wave - min
			if(m[period].min_global_wave==null || data[period][i].scrobbles<m[period].min_global_wave){
				m[period].min_global_wave = data[period][i].scrobbles;
			};
			//global - wave - max
			if(m[period].max_global_wave==null || data[period][i].scrobbles>m[period].max_global_wave){
				m[period].max_global_wave = data[period][i].scrobbles;
			};
			//global - bar - min
			if(helper.isOdd(i)){
				if(m[period].min_global_bar==null || (data[period][i].scrobbles + data[period][i-1].scrobbles)<m[period].min_global_bar){
					m[period].min_global_bar = data[period][i].scrobbles + data[period][i-1].scrobbles;
				};
			};
			//global - bar - max
			if(helper.isOdd(i)){
				if(m[period].max_global_bar==null || (data[period][i].scrobbles + data[period][i-1].scrobbles)>m[period].max_global_bar){
					m[period].max_global_bar = data[period][i].scrobbles + data[period][i-1].scrobbles;
				};
			};
			//totals
			m[period].total_user += data[period][i].user_scrobbles;
			m[period].total_global += data[period][i].scrobbles;
		};
		//MATHS - RANGE
			//user - wave
			m[period].range_user_wave = m[period].max_user_wave-m[period].min_user_wave;
			//user - bar
			m[period].range_user_bar = m[period].max_user_bar-m[period].min_user_bar;
			//global - wave
			m[period].range_global_wave = m[period].max_global_wave-m[period].min_global_wave;
			//global - bar
			m[period].range_global_bar = m[period].max_global_bar-m[period].min_global_bar;
		//MATHS - Y STEP
			//user - wave
			m[period].step_user_wave = m[period].range_user_wave/m.wave_dif_h1;
			if(m[period].step_user_wave <= 0){
				m[period].step_user_wave = 1;
			};
			//user - bar
			m[period].step_user_bar = m[period].range_user_bar/m.bar_dif_h;
			if(m[period].step_user_bar <= 0){
				m[period].step_user_bar = 1;
			};
			//global - wave
			m[period].step_global_wave = m[period].range_global_wave/m.wave_dif_h2;
			if(m[period].step_global_wave <= 0){
				m[period].step_global_wave = 1;
			};
			//global - bar
			m[period].step_global_bar = m[period].range_global_bar/m.bar_dif_h;
			if(m[period].step_global_bar <= 0){
				m[period].step_global_bar = 1;
			};
		//CALCULATE BAR DIMENSIONS
		var count = 0;
		for(var i in data[period]){
			//WAVE
				//user - height
				var h1 = ((data[period][i].user_scrobbles - m[period].min_user_wave) / m[period].step_user_wave) + m.wave_min_h1;
				//global - height
				var h2 = ((data[period][i].scrobbles - m[period].min_global_wave) / m[period].step_global_wave) + m.wave_min_h2;
				//user - top
				var t1 = ((m.wave_max_h1-h1)/2) + ((m.wave_max_h2-m.wave_max_h1)/2);
				//global - top
				var t2 = ((m.wave_max_h2-h2)/2);
				//save
				data2[period][i] = {
					h1: h1,
					h2: h2,
					t1: t1,
					t2: t2
				};
			//BARS
				if(helper.isOdd(count)){
					//user - height
					var h1_a = data[period][i].user_scrobbles + data[period][i-1].user_scrobbles;
					if(h1_a <= 0){
						var h1 = m.bar_min_h;
					}else{
						var h1 = ((h1_a - m[period].min_user_bar) / m[period].step_user_bar) + m.bar_min_h;
					};
					//global - height
					var h2_a = data[period][i].scrobbles + data[period][i-1].scrobbles;
					if(h2_a <= 0){
						var h2 = m.bar_min_h;
					}else{
						var h2 = ((h2_a - m[period].min_global_bar) / m[period].step_global_bar) + m.bar_min_h;
					};
					//save
					data3[period].push({
						h1: h1,
						h2: h2,
						val1: h1_a,
						val2: h2_a
					});
				};
			//COUNT
			count++;
		};
		//FINISH
		m[period].got = true;
	};
	
	
	/****************************** ANIMATE ******************************/
	//SCROLL INIT
	function scroll_init(){
		m.timeout_animate = setTimeout(function(){
			output(m.init_period);
		},1000);
	};
	
	
	/****************************** OUTPUT ******************************/
	function output(period){
		if(m[period].got == true){
			//OUTPUT AXIS
			output_axis_x(period);
			output_axis_y_user_wave(period);
			output_axis_y_global_wave(period);
			//OUTPUT WAVE
			output_wave(period);
			output_bars(period);
			//OUTPUT TOTALS
			m.trenduser_span.text(helper.commas(m[period].total_user));
			m.trendglobal_span.text(helper.commas(m[period].total_global));
			//BUTTONS
			m.btn_trendnav.removeClass('current');
			m['btn_trendnav_'+period].addClass('current');
		}else{
			clearTimeout(m.timeout_output);
			m.timeout_output = setTimeout(function(){
				output(period);
			},50);
		};
	};
	
	
	/****************************** OUTPUT - AXIS ******************************/
	function output_axis_x(period){
		var step = m.length/10;
		var count = 0;
		var count2 = 0;
		var output = '';
		//GET LABELS
		for(var i in data[period]){
			var i2 = parseInt(i)+1;
			if(i2==Math.round(count) || count==0){
				count = count + step + step;
				count2++;
				var d1 = data[period][i].date;
				var d2 = d1.split('-');
				var xy = parseInt(d2[0]);
				var xy2 = xy.toString().split('');
				var xm = parseInt(d2[1]);
				var xd = parseInt(d2[2]);
				if(period=='day'){
					var date = xd + ' ' + locale.month('en',xm-1);
				}else if(period=='week'){
					var date = xd + ' ' + locale.month('en',xm-1) + ' \'' + xy2[2] + xy2[3];
				}else{
					var date = locale.month(m.locale,xm-1) + ' ' + xy;
				};
				output += '<div class="axislabel x'+count2+'">'+date+'<div class="axismarker"></div></div>';
			};
		};
		//OUTPUT AXIS
		m.axis.html(output);
		m.axis2.html(output);
	};
	function output_axis_y_user_wave(period){
		m.axis_label1.html(helper.commas(m[period].max_user_wave));
	};
	function output_axis_y_global_wave(period){
		m.axis_label2.html(helper.commas(m[period].max_global_wave));
	};
	
	
	/****************************** OUTPUT - WAVE ******************************/
	function output_wave(period){
		//User
		var count = 0;
		var delay = 0;
		m.wave_bars.each(function(){
			if(typeof(data2[period][count])!=='undefined'){
				$(this).css({
					height: data2[period][count].h1,
					top: data2[period][count].t1,
					'-webkit-transitionDelay': delay+'s',
					'-moz-transitionDelay': delay+'s',
					'-o-transitionDelay': delay+'s',
					'transitionDelay': delay+'s'
				}).attr('data-val',data[period][count].user_scrobbles).attr('data-date',data[period][count].date);
				delay += m.animation_interval;
				count++;
			};
		});
		//Global
		var count = 0;
		var delay = 0;
		m.wave2_bars.each(function(){
			if(typeof(data2[period][count])!=='undefined'){
				$(this).css({
					height: data2[period][count].h2,
					top: data2[period][count].t2,
					'-webkit-transitionDelay': delay+'s',
					'-moz-transitionDelay': delay+'s',
					'-o-transitionDelay': delay+'s',
					'transitionDelay': delay+'s'
				}).attr('data-val',data[period][count].scrobbles).attr('data-date',data[period][count].date);
				delay += m.animation_interval;
				count++;
			};
		});
	};
	
	
	/****************************** OUTPUT - BAR CHART ******************************/
	function output_bars(period){
		//User
		var count = 0;
		var delay = m.animation_interval/2;
		m.barchart_bars.each(function(){
			if(typeof(data3[period][count])!=='undefined'){
				$(this).css({
					height: data3[period][count].h1,
					'-webkit-transitionDelay': delay+'s',
					'-moz-transitionDelay': delay+'s',
					'-o-transitionDelay': delay+'s',
					'transitionDelay': delay+'s'
				}).attr('data-val',data3[period][count].val).attr('data-date',data3[period][count].date);
				delay += m.animation_interval;
				count++;
			};
		});
		//Global
		var count = 0;
		var delay = 0;
		m.barchart2_bars.each(function(){
			if(typeof(data3[period][count])!=='undefined'){
				$(this).css({
					height: data3[period][count].h2,
					'-webkit-transitionDelay': delay+'s',
					'-moz-transitionDelay': delay+'s',
					'-o-transitionDelay': delay+'s',
					'transitionDelay': delay+'s'
				}).attr('data-val',data3[period][count].val).attr('data-date',data3[period][count].date);
				delay += m.animation_interval;
				count++;
			};
		});
	};
	
	
	/****************************** TOGGLE GRAPHS ******************************/
	function toggle(direction){
		if(m.current == 'wave'){
			m.current = 'bar';
			var prev = m.trendwave_wrap;
			var next = m.trendbar_wrap;
		}else{
			m.current = 'wave';
			var prev = m.trendbar_wrap;
			var next = m.trendwave_wrap;
		};
		if(direction == 'left'){
			next.stop(true,true).css({
				opacity: 0,
				left: '-105%'
			}).animate({
				opacity: 1,
				left: 0
			});
			prev.stop(true,true).animate({
				opacity: 0,
				left: '105%'
			});
		}else{
			next.stop(true,true).css({
				opacity: 0,
				left: '105%'
			}).animate({
				opacity: 1,
				left: 0
			});
			prev.stop(true,true).animate({
				opacity: 0,
				left: '-105%'
			});
		};
	};
	
	
	/****************************** RETURN ******************************/
	return w;
	
	
});

